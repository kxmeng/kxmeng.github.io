{
    "name": "LUS Seating Plans",
    "description": "Seating plan on iSAMS",
    "version": "1.0",
    "manifest_version": 3,
    "content_scripts": [
      {
        "matches": [
            "http://*.isams.latymer-upper.org/*",
            "https://*.isams.latymer-upper.org/*"
            ],
        "js": ["script.js"],
        "run_at": "document_idle"
      }
    ],
    "permissions": ["activeTab", "scripting"],
    "icons": {
      "16": "ico16.png"
    }
}
